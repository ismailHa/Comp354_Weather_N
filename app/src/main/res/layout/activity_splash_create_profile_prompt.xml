<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ca.concordia.comp354mn.project.ui.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:text="No profile found."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" android:id="@+id/textView"
        app:layout_constraintVertical_bias="0.26999998"/>
    <Button
        android:text="@string/do_initial_setup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button" app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="186dp" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="148dp" android:layout_marginEnd="148dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="277dp"
        android:onClick="setupProfile"/>


</android.support.constraint.ConstraintLayout>